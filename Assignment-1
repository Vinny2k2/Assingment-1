SELECT * FROM org.worker;
SELECT FIRST_NAME FROM org.worker;
SELECT DEPARTMENT FROM org.worker; 
SELECT left(FIRST_NAME,3) FROM org.worker; 
SELECT * FROM org.worker NTorder by FIRST_NAME ASC AND DEPARTMENT DESC;
SELECT * FROM org.worker
where DEPARTMENT = Admin;
 SELECT distinct DEPARTMENT FROM org.worker, lengthofDEPartment;
SELECT * FROM org.worker
WHERE SALARY >= 50000 and SALARY <= 100000;
SELECT * FROM org.worker
WHERE SALARY >= 50000 and SALARY <= 100000;
